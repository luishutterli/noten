{
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
    "formatter": {
        "enabled": true,
        "indentStyle":"space",
        "indentWidth": 4,
        "lineEnding":"lf"
    },
    "javascript": {
        "formatter": {
            "enabled": true,
            "bracketSameLine": true,
            "bracketSpacing": true,
            "quoteStyle":"double",
            "semicolons":"always",
            "lineWidth": 135
        }
    }
}